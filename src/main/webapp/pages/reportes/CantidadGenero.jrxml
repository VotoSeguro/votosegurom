select 0 as males, count(*) as Females
from (SELECT DISTINCT v.idvotante FROM tblvoto v inner join tblvotante vo on vo.IDVOTANTE = v.IDVOTANTE where vo.GENERO = 'F' and v.IDPERIODO =$P{idperiodo} ))]]>
